{
    "name": "Zigbee2MQTT-P",
    "version": "0.2.0",
    "slug": "zigbee2mqtt_p",
    "description": "Proxy for externally running Zigbee2MQTT-P",
    "url": "https://github.com/DmitryS-hub/zigbee2mqtt-r-e/zigbee2mqtt-p",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:zigbee",
    "map": [
      "config:ro"
    ],
    "ports": {
      "7788/tcp": null
    },
    "ports_description": {
      "7788/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    },
    "image": "zigbee2mqtt/zigbee2mqtt-proxy-{arch}"
}
